            "enum": [
              "1050SBBR015"
            ]
          },
          "queryDate": {
            "type": "string",
            "format": "date-time",
            "enum": [
              "ISO 8601",
              "yyyy-MM-dd'T'HH:mm:ssZZ"
            ]
          },
          "effectiveDate": {
            "type": "string",
            "format": "date-time",
            "description": "The date set to fees or charges be in effect.",
            "enum": [
              "ISO 8601",
              "yyyy-MM-dd'T'HH:mm:ssZZ"
            ]
          },
          "icao": {
            "type": "string",
            "description": "The ICAO code of the aerodrome.",
            "enum": [
              "SBXX"
            ]
          },
          "group": {
            "type": "string",
            "description": "The aviation group to which the data applies.",
            "enum": [
              "I - Commercial Air Transport",
              "II - General Aviation"
            ]
          },
          "nat": {
            "type": "string",
            "description": "The nature of the arrival flight.",
            "enum": [
              "dom - domestic",
              "intl - international"
            ]
          },
          "chargename": {
            "type": "string",
            "description": "The type of fee or charge.",
            "enum": [
              "Boarding - Tarifa de Embarque",
              "Connection - Tarifa de Conexão",
              "Landing - Tarifa de Pouso",
              "ShortStay - Tarifa de Permanência no Pátio de Manobras",
              "LongStay - Tarifa de Permanência na Área de Estadia"
            ]
          },
          "formula": {
            "type": "string",
            "description": "A reference to the way due values are calculated for an aircraft operation. May be a linear equation with the constant “a” and the coefficient “b” that multiplies the variable, generally the MTOW, or a “band type” structure where due values are obtained by finding the range (band) in which the MTOW is included.",
            "enum": [
              "abx - functions like a + b.x linear equations",
              "band - by range of MTOW",
              "MTOW - metric-ton",
              "Time - hours"
            ]
          },
          "fixvalue": {
            "type": "number",
            "format": "float",
            "description": "Applies only when the formula is “abx”. The constant parameter “a” in the linear equation which determines the fixed component of the due value.",
            "enum": [
              "BRL (Brazilian Real)"
            ]
          },
          "varvalue": {
            "type": "number",
            "format": "float",
            "description": "Applies only when the formula is “abx”. The coefficient “b” in the linear equation which should be multiplied by the variable MTOW to determine the variable component of the due value.",
            "enum": [
              "BRL (Brazilian Real)"
            ]
          },
          "upband": {
            "type": "integer",
            "description": "Applies only when the formula is “band”. The greater or equal value to be compared to an aircraft’s MTOW to obtain which charge applies.",
            "enum": [
              "MTOW - metric-ton"
            ]
          },
          "charge": {
            "type": "number",
            "format": "float",
            "description": "Applies only when the formula is “band”. The value applied to the specified upband.",
            "enum": [
              "BRL (Brazilian Real)"
            ]
          }
